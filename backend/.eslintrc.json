{
  "env": {
    "node": true,
    "es2021": true,
    "es6": true
  },
  "engines": {
    "node": "detect"
  },
  "extends": ["eslint:recommended", "plugin:node/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
